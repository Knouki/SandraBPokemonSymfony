<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SandraPokemonBundle\Entity\Badges" table="badges">
    <indexes>
      <index name="FK_badges_id_dresseur_maitre" columns="id_dresseur_maitre"/>
      <index name="FK_badges_id_zones" columns="id_zones"/>
    </indexes>
    <id name="idBadges" type="integer" column="id_badges">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idDresseurMaitre" target-entity="Dresseurs" fetch="LAZY">
      <join-columns>
        <join-column name="id_dresseur_maitre" referenced-column-name="id_dresseur"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idZones" target-entity="Zones" fetch="LAZY">
      <join-columns>
        <join-column name="id_zones" referenced-column-name="id_zones"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="idDresseur" target-entity="Dresseurs" mapped-by="idBadges" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
