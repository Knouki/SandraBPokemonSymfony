<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SandraPokemonBundle\Entity\Positions" table="positions">
    <indexes>
      <index name="FK_positions_id_zones" columns="id_zones"/>
    </indexes>
    <id name="idPositions" type="integer" column="id_positions">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="x" type="integer" column="x" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="y" type="integer" column="y" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="idZones" target-entity="Zones" fetch="LAZY">
      <join-columns>
        <join-column name="id_zones" referenced-column-name="id_zones"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
