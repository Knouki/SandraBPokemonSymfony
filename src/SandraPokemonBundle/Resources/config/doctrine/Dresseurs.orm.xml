<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SandraPokemonBundle\Entity\Dresseurs" table="dresseurs">
    <indexes>
      <index name="FK_dresseurs_id_badges" columns="id_badges1"/>
    </indexes>
    <id name="idDresseur" type="integer" column="id_dresseur">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idBadges1" target-entity="Badges" fetch="LAZY">
      <join-columns>
        <join-column name="id_badges1" referenced-column-name="id_badges"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="idBadges" target-entity="Badges" inversed-by="idDresseur" fetch="LAZY">
      <join-table name="gagne">
        <join-columns>
          <join-column name="id_dresseur" referenced-column-name="id_dresseur"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="id_badges" referenced-column-name="id_badges"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
