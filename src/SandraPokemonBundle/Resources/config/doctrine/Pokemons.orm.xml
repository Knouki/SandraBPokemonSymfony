<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SandraPokemonBundle\Entity\Pokemons" table="pokemons">
    <indexes>
      <index name="FK_pokemons_id_attaques_1" columns="id_attaques_1"/>
      <index name="FK_pokemons_id_attaques_2" columns="id_attaques_2"/>
      <index name="FK_pokemons_id_attaques_3" columns="id_attaques_3"/>
      <index name="FK_pokemons_id_attaques_4" columns="id_attaques_4"/>
      <index name="FK_pokemons_id_dresseur" columns="id_dresseur"/>
      <index name="FK_pokemons_id_type_pokemons" columns="id_type_pokemons"/>
    </indexes>
    <id name="idPokemons" type="integer" column="id_pokemons">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="surnom" type="string" column="surnom" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="niveau" type="integer" column="niveau" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="capture" type="date" column="capture" nullable="false"/>
    <many-to-one field="idDresseur" target-entity="Dresseurs" fetch="LAZY">
      <join-columns>
        <join-column name="id_dresseur" referenced-column-name="id_dresseur"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTypePokemons" target-entity="TypeDePokemons" fetch="LAZY">
      <join-columns>
        <join-column name="id_type_pokemons" referenced-column-name="id_type_pokemons"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAttaques4" target-entity="Attaques" fetch="LAZY">
      <join-columns>
        <join-column name="id_attaques_4" referenced-column-name="id_attaques"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAttaques3" target-entity="Attaques" fetch="LAZY">
      <join-columns>
        <join-column name="id_attaques_3" referenced-column-name="id_attaques"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAttaques2" target-entity="Attaques" fetch="LAZY">
      <join-columns>
        <join-column name="id_attaques_2" referenced-column-name="id_attaques"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAttaques1" target-entity="Attaques" fetch="LAZY">
      <join-columns>
        <join-column name="id_attaques_1" referenced-column-name="id_attaques"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
