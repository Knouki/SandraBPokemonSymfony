{% extends 'base.html.twig' %}

{% block body %}
    <h1>TypeDePokemons</h1>

    <table>
        <tbody>
            <tr>
                <th>Nom</th>
                <td>{{ typeDePokemon.nom }}</td>
            </tr>
            <tr>
                <th>Attaque</th>
                <td>{{ typeDePokemon.attaque }}</td>
            </tr>
            <tr>
                <th>Attaquespe</th>
                <td>{{ typeDePokemon.attaqueSpe }}</td>
            </tr>
            <tr>
                <th>Defence</th>
                <td>{{ typeDePokemon.defence }}</td>
            </tr>
            <tr>
                <th>Defencespe</th>
                <td>{{ typeDePokemon.defenceSpe }}</td>
            </tr>
            <tr>
                <th>Vitesse</th>
                <td>{{ typeDePokemon.vitesse }}</td>
            </tr>
            <tr>
                <th>Pv</th>
                <td>{{ typeDePokemon.pv }}</td>
            </tr>
            <tr>
                <th>Idtypepokemons</th>
                <td>{{ typeDePokemon.idTypePokemons }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('typedepokemons_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('typedepokemons_edit', { 'id': typeDePokemon.idTypePokemons }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
